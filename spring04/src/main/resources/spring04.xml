<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<context:annotation-config/>
	<bean id="consoleLogger" class="com.spring.sh4.spring04.ConsoleLoggerImpl"/>
	<bean id="fileLogger" class="com.spring.sh4.spring04.FileLoggerImpl"/>
	<bean id="b1" class="com.spring.sh4.spring04.BoxImpl" scope="prototype">
		<property name="length" value="10"/>
		<property name="breadth" value="10"/>
		<property name="height" value="10"/>
	</bean>
</beans>
